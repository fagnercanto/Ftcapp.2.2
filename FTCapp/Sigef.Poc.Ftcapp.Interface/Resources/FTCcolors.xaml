    <SolidColorBrush x:Key="TextBlockBackGroundTitleSuccess" Color="{StaticResource GreenLightColor}" />
    <SolidColorBrush x:Key="TextBlockBackGroundTitleWarnning" Color="{StaticResource YellowLightColor}" />
