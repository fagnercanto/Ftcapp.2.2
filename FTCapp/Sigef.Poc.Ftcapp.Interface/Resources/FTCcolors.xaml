    <SolidColorBrush x:Key="TextBlockForengroundTitle" Color="{StaticResource LightColor}" />
    <SolidColorBrush x:Key="TextBlockForengroundTitleFail" Color="{StaticResource LightColor}" />
    <SolidColorBrush x:Key="TextBlockForengroundTitleSuccess" Color="{StaticResource LightColor}" />
    <SolidColorBrush x:Key="TextBlockForengroundTitleWarnning" Color="{StaticResource LightColor}" />
    <SolidColorBrush x:Key="TextBlockForengroundTitleNormal" Color="{StaticResource LightColor}" />

    <!--  BUTTON  -->

    <SolidColorBrush x:Key="ButtonBackGround" Color="{StaticResource BlueColor}" />
    <SolidColorBrush x:Key="ButtonBackGroundFail" Color="{StaticResource RedColor}" />
    <SolidColorBrush x:Key="ButtonBackGroundSuccess" Color="{StaticResource GreenColor}" />
    <SolidColorBrush x:Key="ButtonBackGroundWarnning" Color="{StaticResource YellowColor}" />
    <SolidColorBrush x:Key="ButtonBackGroundNormal" Color="{StaticResource TaupeColor}" />

    <SolidColorBrush x:Key="ButtonBorder" Color="{StaticResource BlueLightColor}" />
