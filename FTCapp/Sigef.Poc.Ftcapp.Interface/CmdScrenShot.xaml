<UserControl
    x:Class="Sigef.Poc.Ftcapp.Interface.CmdScrenShot"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <ScrollViewer VerticalScrollBarVisibility="Auto">
        <StackPanel>


            <StackPanel>

                <Image x:Name="Image" Source="{Binding Source={StaticResource MVBaseViewModel}, Path=VM.SelectedCaso.SelectedResultado.SelectedComand.ScShot, Mode=TwoWay}" />

            </StackPanel>
        </StackPanel>
    </ScrollViewer>
</UserControl>
